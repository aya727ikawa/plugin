{
    "manifest_version": 2,
    "name": "Resume Wallet",
    "version": "1.0",
    "description": "Apply for jobs instantly using your resume wallet.",
    "icons": {
      "16": "./icons/dog.png",
      "48": "./icons/pup.png",
      "128": "./icons/cat.png"
    },
    "permissions": [
      "activeTab",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://developer.chrome.com/docs/extensions/*",
          "https://developer.chrome.com/docs/webstore/*"
        ]
      }
    ],
    "browser_action": {
      "default_icon": {
        "16": "./icons/dog.png",
        "48": "./icons/pup.png",
        "128": "./icons/cat.png"
      },
      "default_popup": "popup.html"
    }
  }
  
  
  
